{
    "name": "activecollab/bootstrap",
    "description": "All packages and code that is needed to glue them together for a typical PHP project",
    "keywords": [
        "project", "bootstrap"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "/Users/ilija/Dropbox/Projects/Components/Controller"
        },
        {
            "type": "vcs",
            "url": "/Users/ilija/Dropbox/Projects/Components/DatabaseStructure"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "activecollab/authentication": "^4.0",
        "activecollab/baseline": "^1.0",
        "activecollab/controller": "dev-next",
        "activecollab/databasemigrations": "^3.0",
        "activecollab/databasestructure": "dev-next",
        "activecollab/eventsdispatcher": "^1.0",
        "activecollab/templateengine": "^2.0",
        "activecollab/utils": "^1.0",
        "php-di/slim-bridge": "^3.0",
        "slim/http-cache": "~0.3.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.0||^4.0",
        "symfony/console": "^4.0||^5.0",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^2.2.0",
        "zendframework/zend-diactoros": "^2.2"
    },
    "suggest": {
        "activecollab/jobsqueue": "Queue for background, delayed, parallel or remote jobs",
        "graylog2/gelf-php": "Log to Graylog2 using GELF",
        "twig/twig": "Twig templating engine"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\Test\\": "test/src"
        }
    }
}
