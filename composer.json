{
    "name": "activecollab/bootstrap",
    "description": "All packages and code that is needed to glue them together for a typical PHP project",
    "keywords": [
        "project", "bootstrap"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "activecollab/authentication": "^3.0",
        "activecollab/baseline": "^1.0",
        "activecollab/controller": "^1.0",
        "activecollab/databasemigrations": "^2.0",
        "activecollab/databasestructure": "^1.0",
        "activecollab/logger": "^1.0",
        "activecollab/utils": "^1.0",
        "slim/http-cache": "~0.3.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.0",
        "symfony/console": "^4.0||^5.0",
        "vlucas/phpdotenv": "^2.2.0"
    },
    "suggest": {
        "activecollab/jobsqueue": "Queue for background, delayed, parallel or remote jobs",
        "graylog2/gelf-php": "Log to Graylog2 using GELF"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\Test\\": "test/src"
        }
    }
}
