{
    "name": "activecollab/bootstrap",
    "description": "All packages and code that is needed to glue them together for a typical PHP project",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "slim/http-cache": "~0.3.0",
        "activecollab/authentication": "dev-master",
        "activecollab/datevalue": "~1.0.0",
        "activecollab/databaseconnection": "dev-next",
        "activecollab/databasemigrations": "dev-master",
        "activecollab/databaseobject": "dev-master",
        "activecollab/databasestructure": "dev-master",
        "activecollab/filesystem": "^0.1.0",
        "activecollab/jobsqueue": "dev-next",
        "activecollab/promises": "^1.0.0",
        "activecollab/user": "~1.0",
        "graylog2/gelf-php": "~1.4.0",
        "ircmaxell/random-lib": "~1.1",,
        "monolog/monolog": "~1.17.0",
        "slim/php-view": "^2.0",
        "slim/slim": "3.1.0",
        "symfony/console": "~2.7",
        "vlucas/phpdotenv": "~2.1"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\": "src",
            "ActiveCollab\\Bootstrap\\Test\\": "test/src"
        }
    }
}
