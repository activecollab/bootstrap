{
    "name": "activecollab/bootstrap",
    "description": "All packages and code that is needed to glue them together for a typical PHP project",
    "keywords": [
        "project", "bootstrap"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "activecollab/authentication": "^1.0.0",
        "activecollab/controller": "^0.10.0",
        "activecollab/databasemigrations": "^1.0.0",
        "activecollab/databasestructure": "^0.10.0",
        "graylog2/gelf-php": "~1.4.0",
        "monolog/monolog": "~1.17.0",
        "slim/http-cache": "~0.3.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.3.0",
        "symfony/console": "^3.0.0",
        "vlucas/phpdotenv": "^2.2.0"
    },
    "suggest": {
        "activecollab/jobsqueue": "Queue for background, delayed, parallel or remove jobs"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\Bootstrap\\": "src",
            "ActiveCollab\\Bootstrap\\Test\\": "test/src"
        }
    }
}
